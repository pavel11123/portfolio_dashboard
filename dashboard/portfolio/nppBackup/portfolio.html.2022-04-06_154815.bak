		<script>
			var acc = document.getElementsByClassName("accordion__btn");
			var i;
			for (i = 0; i < acc.length; i++){
				acc[i].addEventListener("click", function(){
					this.classList.toggle("active");
					var accordion__menu = this.previousElementSibling;
					if (accordion__menu.style.maxWidth){
						accordion__menu.style.maxWidth = null;
						}else {
						accordion__menu.style.maxWidth = accordion__menu.scrollWidth + "px";
					}
				});
			}
		</script>
		
		<script>
			var acc = document.getElementsByClassName("accordion__main");
			var i;
			for (i = 0; i < acc.length; i++){
				acc[i].addEventListener("click", function(){
					this.classList.toggle("active1");
					var accordion__text = this.nextElementSibling;
					if (accordion__text.style.maxHeight){
						accordion__text.style.maxHeight = null;
						}else {
						accordion__text.style.maxHeight = accordion__text.scrollHeight + "px";
					}
				});
			}
		</script>
		
